{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "type": {
          "type": "string"
        },
        "partnerUserIds": {
          "type": "array",
          "items": {
          	"type": "integer"
          	}
        },
        "ssoGuid": {
          "type": "string"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "id",
        "type",
        "partnerUserIds",
        "ssoGuid"
      ]
    },
    "token": {
      "type": "string"
    }
  },
  "required": [
    "user",
    "token"
  ]
}